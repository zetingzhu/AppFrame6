<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SelectableTextView">
        <attr name="textJustify" format="boolean" />
        <attr name="forbiddenActionMenu" format="boolean" />
        <attr name="textHeightColor" format="color" />
    </declare-styleable>

    <declare-styleable name="DragSortListView">
        <attr name="collapsed_height" format="dimension" />
        <attr name="drag_scroll_start" format="float" />
        <attr name="max_drag_scroll_speed" format="float" />
        <attr name="float_background_color" format="color" />
        <attr name="remove_mode">
            <enum name="clickRemove" value="0" />
            <enum name="flingRemove" value="1" />
        </attr>
        <attr name="track_drag_sort" format="boolean" />
        <attr name="float_alpha" format="float" />
        <attr name="slide_shuffle_speed" format="float" />
        <attr name="remove_animation_duration" format="integer" />
        <attr name="drop_animation_duration" format="integer" />
        <attr name="drag_enabled" format="boolean" />
        <attr name="sort_enabled" format="boolean" />
        <attr name="remove_enabled" format="boolean" />
        <attr name="drag_start_mode">
            <enum name="onDown" value="0" />
            <enum name="onMove" value="1" />
            <enum name="onLongPress" value="2" />
        </attr>
        <attr name="drag_handle_id" format="integer" />
        <attr name="fling_handle_id" format="integer" />
        <attr name="click_remove_id" format="integer" />
        <attr name="use_default_controller" format="boolean" />
    </declare-styleable>


    <declare-styleable name="CircleImageview">
        <attr name="border_thickness" format="dimension" />
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="corner_radius" format="dimension" />
        <attr name="mutate_background" format="boolean" />
        <attr name="oval" format="boolean" />
        <attr name="android:scaleType" />
        <attr name="circle_style">
            <enum name="circle" value="0"></enum>
            <enum name="round" value="1"></enum>
        </attr>
        <attr name="roundradius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="UnderLineTextView">
        <attr name="lineColor" format="color" />
        <attr name="mlineHeight" format="dimension" />
        <attr name="mlineWidth" format="dimension" />
        <attr name="upOrunder">
            <enum name="under" value="0" />
            <enum name="up" value="1" />
            <enum name="left" value="2" />
            <enum name="right" value="3" />
        </attr>
        <attr name="fitsPadding" format="boolean" />
        <attr name="isLineEnable" format="boolean" />
        <attr name="isLineEnableWhileSelected" format="boolean" />
        <attr name="isBlodWhileSelected" format="boolean" />
        <attr name="isDrawRoundLine" format="boolean" />
        <attr name="roundRadius" format="dimension" />
    </declare-styleable>
    <declare-styleable name="RoundProgressBar">
        <attr name="roundColor" format="color" />
        <attr name="roundProgressColor" format="color" />
        <attr name="roundWidth" format="dimension"></attr>
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="max" format="integer"></attr>
        <attr name="textIsDisplayable" format="boolean"></attr>
        <attr name="direction">
            <enum name="clock_wise" value="0"></enum>
            <enum name="antic_lock_wise" value="1"></enum>
        </attr>
        <attr name="style">
            <enum name="STROKE" value="0"></enum>
            <enum name="FILL" value="1"></enum>
        </attr>
        <attr name="angle">
            <enum name="angle_0" value="0"></enum>
            <enum name="angle_45_1" value="-45"></enum>
            <enum name="angle_90" value="90"></enum>
            <enum name="angle_90_1" value="-90"></enum>
            <enum name="angle_270" value="270"></enum>
            <enum name="angle_300" value="-300"></enum>
        </attr>
    </declare-styleable>
    <declare-styleable name="RoundImageView">
        <attr name="round_radius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ShapeTextView">
        <attr name="ShapeTextView_corner" format="reference|dimension" />
        <attr name="ShapeTextView_solid" format="color" />
    </declare-styleable>

    <declare-styleable name="CornerTextView">
        <attr name="CornerTextView_corner" format="reference|dimension" />
        <attr name="CornerTextView_borderColor" format="color" />
        <attr name="CornerTextView_solidColor" format="color" />
        <attr name="CornerTextView_strokeWidth" format="reference|dimension" />
    </declare-styleable>

    <declare-styleable name="MissionProgressBar">
        <attr name="MissionProgressBar_corner" format="reference|dimension" />
        <attr name="MissionProgressBar_backgroundColor" format="color" />
        <attr name="MissionProgressBar_progressDrawable" format="reference" />
        <attr name="MissionProgressBar_progressColor" format="color" />
    </declare-styleable>

    <declare-styleable name="DashView">
        <attr name="DashView_dashWidth" format="dimension" />
        <attr name="DashView_lineWidth" format="dimension" />
        <attr name="DashView_lineColor" format="color" />
        <attr name="DashView_dashOrientation" format="integer" />

    </declare-styleable>

    <declare-styleable name="NineGridView">
        <attr name="nine_gv_spacing" format="dimension" />
        <attr name="nine_maxImageNum" format="integer" />
        <attr name="nine_single_image" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CashOutAddCardInputView">
        <attr name="displaylable" format="string" />
        <attr name="displaylable_color" format="color" />
        <attr name="error_color" format="color" />
    </declare-styleable>

    <declare-styleable name="stl_SmartTabLayout">
        <attr name="stl_indicatorAlwaysInCenter" format="boolean" />
        <attr name="stl_indicatorWithoutPadding" format="boolean" />
        <attr name="stl_indicatorInFront" format="boolean" />
        <attr name="stl_indicatorInterpolation" format="enum">
            <enum name="smart" value="0" />
            <enum name="linear" value="1" />
        </attr>
        <attr name="stl_indicatorGravity" format="enum">
            <enum name="bottom" value="0" />
            <enum name="top" value="1" />
            <enum name="center" value="2" />
        </attr>
        <attr name="stl_indicatorPosition" format="enum">
            <enum name="left" value="0" />
            <enum name="center" value="1" />
            <enum name="right" value="2" />
        </attr>
        <attr name="stl_indicatorColor" format="color" />
        <attr name="stl_indicatorColors" format="reference" />
        <attr name="stl_indicatorThickness" format="dimension" />
        <attr name="stl_indicatorWidth" format="dimension">
            <enum name="auto" value="-1" />
        </attr>
        <attr name="stl_indicatorCornerRadius" format="dimension" />
        <attr name="stl_overlineColor" format="color" />
        <attr name="stl_overlineThickness" format="dimension" />
        <attr name="stl_underlineColor" format="color" />
        <attr name="stl_underlineThickness" format="dimension" />
        <attr name="stl_dividerColor" format="color" />
        <attr name="stl_dividerColors" format="reference" />
        <attr name="stl_dividerThickness" format="dimension" />
        <attr name="stl_defaultTabBackground" format="reference" />
        <attr name="stl_defaultTabTextAllCaps" format="boolean" />
        <attr name="stl_defaultTabTextColor" format="color|reference" />
        <attr name="stl_defaultTabTextSize" format="dimension" />
        <attr name="stl_defaultTabTextHorizontalPadding" format="dimension" />
        <attr name="stl_defaultTabTextMinWidth" format="dimension" />
        <attr name="stl_customTabTextLayoutId" format="reference" />
        <attr name="stl_customTabTextViewId" format="reference" />
        <attr name="stl_distributeEvenly" format="boolean" />
        <attr name="stl_clickable" format="boolean" />
        <attr name="stl_titleOffset" format="dimension">
            <enum name="auto_center" value="-1" />
        </attr>
        <attr name="stl_drawDecorationAfterTab" format="boolean" />
    </declare-styleable>


    <declare-styleable name="AppLoadingLayout">
        <attr name="loading_img" format="reference" />
        <attr name="loading_text_color" format="color" />
        <attr name="loading_text" format="string" />
    </declare-styleable>

    <declare-styleable name="CreditCardsView">
        <attr name="credit_card_orientation" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
        <attr name="credit_card_example_show" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ImgUploadView">
        <attr name="img_upload_button_type" format="enum">
            <enum name="album_and_pdf" value="0" />
            <enum name="album" value="1" />
            <enum name="pdf" value="2" />
        </attr>
    </declare-styleable>
    <declare-styleable name="MarqueeView">
        <attr name="mvInterval" format="integer|reference" />
        <attr name="mvAnimDuration" format="integer|reference" />
        <attr name="mvGravity">
            <enum name="left" value="0" />
            <enum name="center" value="1" />
            <enum name="right" value="2" />
        </attr>
        <attr name="mvDirection">
            <enum name="bottom_to_top" value="0" />
            <!--除bottom_to_top 其他的需自行添加动画-->
            <enum name="top_to_bottom" value="1" />
            <enum name="right_to_left" value="2" />
            <enum name="left_to_right" value="3" />
        </attr>
    </declare-styleable>
    <declare-styleable name="MyRatingBar">
        <!--尺寸值-->
        <attr name="starImageSize" format="dimension" />
        <!--星星间距-->
        <attr name="starPadding" format="dimension" />
        <!--星星总数-->
        <attr name="starCount" format="integer" />
        <!--空白的星星资源文件值-->
        <attr name="starEmpty" format="reference" />
        <!--满星资源文件值-->
        <attr name="starFill" format="reference" />
        <!--半星资源文件值-->
        <attr name="starHalf" format="reference" />
        <!--是否可点击boolean值-->
        <attr name="clickable" format="boolean" />
        <!--当前进度float值-->
        <attr name="starStep" format="float" />
        <!--每次进度方式的值，整星还是半星-->
        <attr name="stepSize">
            <enum name="Half" value="0" />
            <enum name="Full" value="1" />
        </attr>
    </declare-styleable>

    <!--签名-->
    <declare-styleable name="SignatureView">
        <!--         画笔颜色-->
        <attr name="penColor" format="color" />
        <!--          画布颜色 reference|color-->
        <attr name="backColor" format="reference|color" />
        <!--         画笔宽度-->
        <attr name="penWidth" format="integer" />
    </declare-styleable>

    <!--  FloatLayout start -->
    <declare-styleable name="QMUIFloatLayout">
        <attr name="android:gravity" />
        <attr name="qmui_childHorizontalSpacing" format="dimension" />
        <attr name="qmui_childVerticalSpacing" format="dimension" />
        <attr name="android:maxLines" />
        <attr name="qmui_maxNumber" format="integer" />
    </declare-styleable>
    <!--  FloatLayout end -->
    <!--recycleView 指示器-->
    <declare-styleable name="HIndicator">
        <attr name="hi_bgColor" format="color" />
        <attr name="hi_indicatorColor" format="color" />
    </declare-styleable>

    <!--倾斜角度文字-->
    <declare-styleable name="SlantedTextView">
        <attr name="slantedTextSize" format="dimension" />
        <attr name="slantedBackgroundColor" format="color" />
        <attr name="slantedText" format="string" />
        <attr name="slantedTextColor" format="color" />
        <!--倾斜的显示文案在水平和垂直方向宽度-->
        <attr name="slantedLength" format="dimension" />
        <attr name="slantedMode" format="enum">
            <!--左上，条-->
            <enum name="left" value="0"></enum>
            <!--右上，条-->
            <enum name="right" value="1"></enum>
            <!--左下，条-->
            <enum name="left_bottom" value="2"></enum>
            <!--右下，条-->
            <enum name="right_bottom" value="3"></enum>
            <!--左上，三角形-->
            <enum name="left_triangle" value="4"></enum>
            <!--右上，三角形-->
            <enum name="right_triangle" value="5"></enum>
            <!--左下，三角形-->
            <enum name="left_bottom_triangle" value="6"></enum>
            <!--右下，三角形-->
            <enum name="right_bottom_triangle" value="7"></enum>
        </attr>
    </declare-styleable>
    <!--背景阴影-->
    <declare-styleable name="ShadowLayout">
        <!--阴影：是否要隐藏阴影 -->
        <attr name="hl_shadowHidden" format="boolean" />
        <!--阴影：阴影颜色-->
        <attr name="hl_shadowColor" format="color" />
        <!--阴影：阴影的扩散范围(也可以理解为扩散程度)-->
        <attr name="hl_shadowLimit" format="dimension" />
        <!--控件区域是否对称，默认是对称。不对称的话，那么控件区域随着阴影区域走-->
        <attr name="hl_shadowSymmetry" format="boolean" />
        <!--阴影：x轴的偏移量-->
        <attr name="hl_shadowOffsetX" format="dimension" />
        <!--阴影：y轴的偏移量-->
        <attr name="hl_shadowOffsetY" format="dimension" />
        <!--阴影：左边是否隐藏阴影-->
        <attr name="hl_shadowHiddenLeft" format="boolean" />
        <!--阴影：右边是否隐藏阴影-->
        <attr name="hl_shadowHiddenRight" format="boolean" />
        <!--阴影：上边是否隐藏阴影-->
        <attr name="hl_shadowHiddenTop" format="boolean" />
        <!--阴影：下面是否隐藏阴影-->
        <attr name="hl_shadowHiddenBottom" format="boolean" />
        <!--圆角：统一大小，其中包括了阴影，shape、背景图、stroke边框圆角-->
        <attr name="hl_cornerRadius" format="dimension" />
    </declare-styleable>
    <!--自定义三角形-->
    <declare-styleable name="TriangleView">
        <!--模式-->
        <attr name="tlv_mode">
            <!--倒三角-->
            <enum name="inverted" value="0" />
            <!--正三角-->
            <enum name="regular" value="1" />
        </attr>
        <!--颜色-->
        <attr name="tlv_color" format="color|reference" />
    </declare-styleable>

    <declare-styleable name="DeemProgressView">
        <attr name="minParticipated" format="integer" />
        <attr name="maxParticipated" format="integer" />
        <attr name="currParticipated" format="integer" />
    </declare-styleable>
</resources>